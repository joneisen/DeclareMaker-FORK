			dim m as new ManualCreation
			m.Show
			Return True
			
		End Function
	#tag EndMenuHandler


	#tag Method, Flags = &h0
		Sub save()
		  dim proj as new RBProject
		  proj.saveAsRBVersion = RBProject.kVersion555
		  for i as Integer = 0 to cls.Ubound
		    proj.addModule cls(i).getClass
		  next
		  proj.addModule AdditionalModule
		  proj.addModule NSObjectManual
		  
		  dim f as FolderItem = GetSaveFolderItem("public/text","GENERATED CLASSES.txt")
		  if f = nil then Return
		  dim tos as TextOutputStream
		  tos = TextOutputStream.Create(f)
		  tos.Write proj.operator_convert
		  tos.Close
		  
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		cls() As DeclareMaker.OBJCClass
	#tag EndProperty


