#tag Events PushButton1
	#tag Event
		Sub Action()
		  save
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Canvas1
	#tag Event
		Sub DropObject(obj As DragItem, action As Integer)
		  if obj.FolderItemAvailable then
		    do
		      try
		        dim tmpCls as new DeclareMaker.OBJCClass(obj.FolderItem)
		        Listbox1.addrow tmpCls.clsName
		        cls.Append tmpCls
		        self.ContentsChanged = True
		      Catch err as NilObjectException
		        MsgBox "Invalid file.  Please make sure the file is an html download of the apple docs and try again"
		      Catch err as OutOfBoundsException
		        MsgBox "Invalid file.  Please make sure the file is an html download of the apple docs and try again"
		      end try
		    loop until not obj.NextItem
		  end if
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  me.AcceptFileDrop("public/html")
		End Sub
	#tag EndEvent
	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.ForeColor = RGB(0,255,0,200)
		  g.FillRect 0,0,g.Width,g.Height
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton2
	#tag Event
		Sub Action()
		  self.ContentsChanged = False
		  Listbox1.DeleteAllRows
		  ReDim cls(-1)
		End Sub
	#tag EndEvent
#tag EndEvents
