#tag WindowCode
	#tag Event
		Function CancelClose(appQuitting as Boolean) As Boolean
		  if self.ContentsChanged then
		    dim md as new MessageDialog
		    md.Message = "Are you sure you want to quit?"
		    md.Explanation = "You have unsaved changes which will be lost."
		    md.ActionButton.Caption = "Cancel"
		    md.AlternateActionButton.Caption = "Quit"
		    md.AlternateActionButton.Visible = True
		    dim m as MessageDialogButton = md.ShowModal
		    if m.Caption = "Cancel" then
		      Return True
		    else
		      Return False
		    end if
		  end if
		End Function
	#tag EndEvent


